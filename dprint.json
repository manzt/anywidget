{
	"typescript": {
		"deno": true,
		"useTabs": true
	},
	"json": {
		"deno": true,
		"useTabs": true
	},
	"markdown": {
		"deno": true
	},
	"excludes": [
		"**/node_modules",
		"**/*-lock.yaml",
		"**/CHANGELOG.md",
		".venv/**/*"
	],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.88.1.wasm",
		"https://plugins.dprint.dev/json-0.17.4.wasm",
		"https://plugins.dprint.dev/markdown-0.15.3.wasm"
	]
}
