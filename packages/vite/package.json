{
	"name": "@anywidget/vite",
	"type": "module",
	"version": "0.1.2",
	"description": "a vite plugin for anywidget",
	"main": "index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "esbuild --format=cjs --platform=node --outfile=dist/index.cjs index.js"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./index.js",
			"require": "./dist/index.cjs"
		}
	},
	"devDependencies": {
		"vite": "^5.2.13"
	},
	"peerDependencies": {
		"vite": "^4.0.0 || ^5.0.0"
	}
}
